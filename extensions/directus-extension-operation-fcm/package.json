{
	"name": "directus-extension-operation-fcm",
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-operation"
	],
	"type": "module",
	"directus:extension": {
		"type": "operation",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"source": {
			"app": "src/app.ts",
			"api": "src/api.ts"
		},
		"host": "^10.3.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "10.3.0",
		"@types/node": "^20.10.6",
		"typescript": "^5.3.3",
		"vue": "^3.4.3"
	},
	"dependencies": {
		"@unhead/vue": "^1.8.9",
		"pinia": "^2.1.7"
	}
}
